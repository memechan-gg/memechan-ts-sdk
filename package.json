{
  "name": "@avernikoz/memechan-ts-sdk",
  "version": "1.0.5",
  "description": "Typescript SDK for Memechan.gg",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "author": "avernikoz <avernikoz@gmail.com>",
  "license": "private",
  "node": "v20",
  "private": false,
  "engineStrict": true,
  "scripts": {
    "test": "jest ./tests/unit",
    "e2e": "jest ./tests/e2e",
    "audit": "yarn audit --groups dependencies",
    "format": "prettier --write src/**/*.ts",
    "lint": "tsc --noEmit && eslint src/**/*.ts",
    "build": "yarn run lint && tsup",
    "build:watch": "tsc --watch",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "deploy": "yarn lint && yarn build && yarn publish --access restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldrin-labs/memechan-ts-sdk.git"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@aws-crypto/sha256-browser": "^5.2.0",
    "@aws-sdk/protocol-http": "^3.374.0",
    "@aws-sdk/signature-v4": "^3.374.0",
    "@avernikoz/memechan-ts-interface": "^1.0.1",
    "@mysten/sui.js": "^0.51.2",
    "bignumber.js": "^9.1.2",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "^5.2.0"
  }
}
